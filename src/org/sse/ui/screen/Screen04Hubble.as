package org.sse.ui.screen{		import org.sse.solarsystem.GoogleMap;	import org.sse.constant.PrivateParams;		import be.colorfield.ui.Application;	import be.colorfield.ui.box.ScrollingBox;	import be.colorfield.ui.screen.IScreen;	import be.colorfield.ui.screen.ScreenAbstract;		/**	 * ECRAN représentant la carte Google Map du Hubble Deep Space Field	 * avec un menu personnalisé	 * + un texte	 */	public class Screen04Hubble extends ScreenAbstract implements IScreen	{		public function Screen04Hubble()		{					}				/**		 * Création des éléments et de leurs options d'affichage		 */		override public function buildElements():void		{				var map = new GoogleMap(GoogleMap.MAP_SKY);			var mapOptions:Object = {x:0,y:0, offSetY:60};			addElement(map,mapOptions);							var sb:ScrollingBox = new ScrollingBox(PrivateParams.COLOR_TITLE);			var sbOptions:Object = {x:0,y:0.5};						sb.setTitle("Le Champ Profond de Hubble");			sb.setContent(new ScreenGoogleMapHubble());						addElement(sb, sbOptions);		}					}}