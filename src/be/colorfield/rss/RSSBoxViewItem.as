package be.colorfield.rss {		import flash.display.MovieClip;	import flash.net.navigateToURL;	import flash.net.URLRequest;	import flash.events.MouseEvent;	/**	 * Item utilisé pour la view RSSBox	 */	public class RSSBoxViewItem extends MovieClip {				private var rssItmURL:String;				public function set rssItemURL(newRssURL:String):void {			rssItmURL = newRssURL;		}				public function RSSBoxViewItem() {			viewPost.addEventListener(MouseEvent.CLICK, handleViewBtnClick);		}				private function handleViewBtnClick(e:MouseEvent):void {			navigateToRssEntry();		}				private function navigateToRssEntry() {			var request:URLRequest = new URLRequest(rssItmURL);			try {            				navigateToURL(request, "_blank");			}			catch (e:Error) {				trace("URL non trouvée ", rssItmURL)			}   		}			}}